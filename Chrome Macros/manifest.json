{
    "manifest_version": 2,
    "name": "Chrome Macros",
    "short_name": "Chrome Macros",
    "version": "1.1",
    "permissions": ["tabs", "http://*/*, https://*/*", "*://*/*", "<all_urls>"],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": ["*://*/*", "http://*/*", "https://*/*", "file://*/*"],
        "js": ["content.js"],
        "run_at": "document_start"
    }],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}