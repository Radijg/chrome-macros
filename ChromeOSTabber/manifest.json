{
    "manifest_version": 2,
    "name": "ChromeOSTabber",
    "version": "1.0",
    "permissions": ["tabs", "http://*/*, https://*/*", "*://*/*", "<all_urls>", "file://*/*"],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*", "*://*/*", "file://*/*"],
        "js": ["ChromeOSTabber.js"],
        "run_at": "document_start"
    }],
    "commands": {
        "anyCommandName": {
            "suggested_key": {
                "default": "Alt+Shift+S"
            },
            "description": "Some description"
        }
    }
}